                <h5 class="card-title">Information</h5>
                <h6 class="card-subtitle mb-2 text-muted">Card Contact</h6>
                <p id="user_email"><strong>Email: </strong><%= user.email %></p>
                <p id="user_phone"><strong>Phone: </strong>[Users Phone]</p>
                <div class="form-group">
                  <h5 class="card-title">User Bio</h5>
                  <p class="card-text" id="user_bio"><%= details.user_bio%>.</p>
                </div>
              </div>
            </div>
            <div class="user_info">
              <div class="username">
                <p><strong>Hello, <%= user.first_name%></strong></p>
              </div>
            </div>
