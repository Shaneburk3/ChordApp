                  alt="Card image cap">
                <div class="card-body">
                  <p id="user_age"><strong>Age: </strong><%= details.user_age%></p>
                  <p id="user_country"><strong>Country: </strong><%= details.user_country%></p>
                  <p id="user_city"><strong>City: </strong><%= details.user_city%></p>
                </div>
            </div>
            <div class="card" style="width: 40rem;">
